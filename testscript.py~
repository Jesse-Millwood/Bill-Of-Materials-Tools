class thing(object):
    def __init__(self, qty, name):
        self.qty = qty
        self.name = name


dict1 = {}
dict2 = {}

dict1 = {'things':[thing(1, 'larry'),
                   thing(1, 'Oliver'),
                   thing(1, 'Fred'),
                   thing(1, 'Amy')]}

dict2 = {'things':[thing(1, 'Jesse'),
                   thing(2, 'Oliver')]}

